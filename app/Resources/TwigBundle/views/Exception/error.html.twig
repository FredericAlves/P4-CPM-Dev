

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Billetterie Musée du Louvre{% endblock %}</title>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="{{ asset('bundles/louvreticketing/plugin/css/bootstrap-datepicker3.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('bundles/louvreticketing/css/main.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('bundles/louvreticketing/css/tunnel.css') }}" type="text/css" />

</head>

<body data-spy="scroll" data-target=".navbar-fixed-top">
<nav class="navbar navbar-default navbar-fixed-top">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('louvre_ticketing_homepage') }}">Musée du Louvre</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
                <li {% if app.request.attributes.get('_route') == 'louvre_ticketing_homepage' %}class="active"{% endif %}><a href="{{ path('louvre_ticketing_homepage') }}">Accueil <span class="sr-only">(current)</span></a></li>
                <li {% if app.request.attributes.get('_route') == 'louvre_ticketing_bookingsteponepage' %}class="active"{% endif %}><a href="{{ path('louvre_ticketing_bookingsteponepage') }}">Billetterie</a></li>
            </ul>
            <form class="navbar-form navbar-left">
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="Recherche">
                </div>
                <button type="submit" class="btn btn-default">Recherche</button>
            </form>

        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
</nav>



<div class="jumbotron">
    <img src="{{ asset('bundles/louvreticketing/images/louvre-panorama.png') }}" alt="Louvre" class="image-jumbotron" >
    <div class="center">Musée du Louvre</div>
</div>

{% for msg in app.session.flashBag.get('error') %}
    <div class="alert alert-danger">
        {{ msg|trans }}
    </div>
{% endfor %}

<div class="container-fluid text-center">

    {% for msg in app.session.flashBag.get('success') %}
        <div class="alert alert-success">
            {{ msg|trans }}
        </div>
    {% endfor %}
    {% for msg in app.session.flashBag.get('error') %}
        <div class="alert alert-danger">
            {{ msg|trans }}
        </div>
    {% endfor %}



    {% block body %}
        <h1>Page non trouvée</h1>

        <p>
            La page demandée n'a pas été trouvée, veuillez vérifier l'URL .
        </p>
        <p><a href="{{ path('louvre_ticketing_homepage') }}" class="btn btn-info" role="button" title="Lien 1">Revenir à l'accueil</a></p>
        </p>

    {% endblock %}





</div>

<footer>
    <section class="footer">
        <div class="container">
            <div class="row">
                <div class="col-lg-4  col-md-4 col-sm-4">
                    <div class="footer_dv">
                        <h4>Informations pratiques</h4>
                        <ul>
                            <li><a href="{{ path('louvre_ticketing_bookingsteponepage') }}">Billetterie</a></li>
                            <li><a href="">Préparer sa visite</a></li>
                            <li><a href="">Louvre, mode d'emploi</a></li>
                            <li><a href="">Plans du musée</a></li>
                            <li><a href="">Comment venir ?</a></li>
                            <li><a href="">Groupes et scolaires</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4  col-md-4 col-sm-4">
                    <div class="footer_dv">
                        <h4>Actualités et Expositions</h4>
                        <ul>
                            <li><a href="">Evénements</a></li>
                            <li><a href="">Conférences et colloques</a></li>
                            <li><a href="">Spectacle vivant et littérature</a></li>
                            <li><a href="">Expositions</a></li>
                            <li><a href="">Films</a></li>
                            <li><a href="">Archives</a></li>


                        </ul>
                    </div>
                </div>
                <div class="col-lg-4  col-md-4 col-sm-4">
                    <div class="footer_dv">
                        <h4>Nous contacter</h4>
                        <p>Service client</p>
                        <p>+ 33 (0)1 40 20 50 50<br>


                        </p></div>
                </div>
            </div>
        </div>
    </section>
</footer>


{% block javascripts %}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
{% endblock %}

</body>
</html>